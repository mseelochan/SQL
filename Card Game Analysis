-- Scan the data 

SELECT * 
FROM card_games;

-- Rank the players scores as "high score", "good score", and "low score" for the game crazy eights. Use greater than or equal to 300 for high score, greater than or equal to 200 for good score and anything below as a low score.

SELECT player_name, score,
CASE WHEN score >= 300 THEN "high score" 
WHEN score >= 200 THEN "good score" 
WHEN score <200 THEN "low score"
END player_scores
FROM card_games
WHERE game_name = "Crazy Eights"
ORDER BY player_name, score;

-- How many players played Pitch on 2015/03/11?

SELECT COUNT(*) AS "amount_of_players"
FROM card_games
WHERE game_name = "Pitch"
AND date_played = "2015/03/11";

-- Which player had the lowest average score after all their days of playing card_games?

SELECT player_name, ROUND(AVG(score)) AS "average_score" 
FROM card_games 
GROUP BY player_name
ORDER BY average_score
LIMIT 1;

-- Which players had a total score of above 1500? 

SELECT player_name, SUM(score) AS "total_score"
FROM card_games
GROUP BY player_name
HAVING total_score > 1500
ORDER BY total_score desc;
